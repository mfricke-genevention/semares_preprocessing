{
    "output": {
        "file_name": "data.csv",
        "sep": ",",
        "quotechar": "",
        "path": "./transformed_input/"
    },
    "input": {
        "sep": ",",
        "quotechar": ""
    },
    "join": {
        "columns": [
            "gene",
            "protein"
        ],
        "name": "ID"
    },
    "name_mapping": true,
    "columns": [
        {
            "maps": [
                {
                    "name": "map_1",
                    "object_type": "sample",
                    "field": [
                        "name"
                    ]
                },
                {
                    "name": "map_2",
                    "object_type": "individual",
                    "field": [
                        "diabetes-grade"
                    ]
                }
            ],
            "name": "{map_1} ({map_2})",
            "input": [
                "abundance",
                "expression"
            ]
        }
    ]
}